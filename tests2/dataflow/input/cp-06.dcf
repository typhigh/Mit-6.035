import printf;
int get_int ( int x ) {
  return x;
}
bool get_bool ( bool x ) {
  return x;
}
void main ( ) {
  int a, c, d;
  bool x;
  a = get_int ( 2 );
  x = get_bool ( true );
  c = 0;
  d = 0;
  c = a;
  if ( x ) {
    d = c;
  }
  printf ( "%d\n", a );
  printf ( "%d\n", c );
  printf ( "%d\n", d );
}
