import printf;
int get_int ( int x ) {
  return x;
}
bool get_bool ( bool x ) {
  return x;
}
int foo ( ) {
  int a, b, c;
  bool x;
  a = get_int ( 7 );
  b = get_int ( 2 );
  x = get_bool ( true );
  c = 0;
  c = a + b;
  if ( x ) {
    return a + b;
  }
  return c - 3;
}
void main ( ) {
  int c;
  c = foo ( );
  printf ( "%d\n", c );
}
