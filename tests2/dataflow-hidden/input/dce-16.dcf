import printf;
int get_int ( int x ) {
  return x;
}
bool get_bool ( bool x ) {
  return x;
}
int foo ( ) {
  int a, b, c, d;
  bool x;
  a = get_int ( 7 );
  b = get_int ( 2 );
  x = get_bool ( false );
  c = a + b;
  d = x ? c : (a * b);
  return d;
}
void main ( ) {
  int c;
  c = foo ( );
  printf ( "%d\n", c );
}
