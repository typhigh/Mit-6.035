import printf;
int get_int ( int x ) {
  return x;
}
bool get_bool ( bool x ) {
  return x;
}
int foo ( ) {
  int a, b, c, i, d;
  bool x;
  a = get_int ( 7 );
  b = get_int ( 2 );
  x = get_bool ( false );
  c = 0;
  d = a+b;
  for (i = 1; i < 20; i += 1) {
    if ( x ) {
      a = 3;
      c = c+i*i* ( a+b );
    }
  }
  c = c + 10* ( a+b );
  return c+d;
}
void main ( ) {
  int c;
  c = foo ( );
  printf ( "%d\n", c );
}
