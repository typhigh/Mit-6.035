import printf;

int x;
void main() {
  bool y;
  x = 7;
  y = true;
  if (y) {
    int x;
    printf("x should be 0; x = %d\n", x);
  }
  if (y) {
    int y;
    y = 3;
    printf("Sum should be 10; x + y = %d\n", x + y);
  }
  if (y) {
    bool y;
    if (y) {
      printf("This shouldnt print\n");
    } else {
      printf("We have mastered shadowing\n");
    }
  }
}