import printf;
int get_int ( int x ) {
  return x;
}
bool get_bool ( bool y ){
  return y;
}
void main ( ) {
  int a, b, c, d, e;
  bool x;
  a = get_int ( 2 );
  b = get_int ( 3 );
  x = get_bool ( true );
  c = a + b;
  if ( x ) {
    int c;
    c = a * b;
    d = c * c;
  }
  e = d + d;
  printf ( "%d\n", e );
}
